set size 1,0.8
set key outside
set grid
set xrange [0;100000]
set yrange [0;100000] 
set title "Comparacoes"
 set xlabel "t" 
 set ylabel "y(x,t)" 
plot  "data" u 1:2 t "y_1[h0.0001]" with lines
replot  "data" u 1:3 t "y_1[h0.01]" with lines
replot  "data" u 1:4 t "y_1[h0.25]" with lines
set terminal png
set output 'imagem.png'
replot
