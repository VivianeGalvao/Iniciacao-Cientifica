g++ -o teste *.cpp -std=c++0x

rm resultS.csv

echo "Codigo Compilado"
f=0
s=0
d=0
m=0
dim=0

echo "PROBLEMA;SEMENTE;DIMENSAO;PSO; PSO-PS">>resultS.csv


for(( f=1; f<22; f++ ))
  do   
   for(( s=1; s<3; s++ ))		  
	  do
	   ./teste $f $s >> resultS.csv
	  done          
  done

gedit resultS.csv

