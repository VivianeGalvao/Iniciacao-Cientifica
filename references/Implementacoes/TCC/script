g++ -o teste *.cpp -std=c++0x

rm README.txt
rm result.csv

echo "Codigo Compilado"
f=0
s=0
d=0
m=0
dim =0

echo "PSO-PS-> Pattern Search com Otimização por Enxame de Partículas" >> README.txt
echo "ES-PS 1 -> Pattern Search com Estratégia Evolutiva(1+1) considerando adaptação do desvio padrão através da técnica 1/5 de sucessos" >> README.txt
echo "ES-PS 2 -> Pattern Search com Estrategia Evolutiva(1+1) considerando atualização do desvia padrão isomórfica a cada iteração" >> README.txt
echo "ES-PS 3 -> Pattern Search com Estrategia Evolutiva(mi + lambda) considerando atualização do desvia padrão isomórfica a cada iteração para cada indivíduo" >> README.txt
echo "ES-PS 4 -> Pattern Search com Estrategia Evolutiva(mi + lambda) considerando atualização do desvia padrão atraves da tecnica 1/5 de sucessos, a cada iteração para cada indivíduo" >> README.txt

#echo "PROBLEMA;SEMENTE;PSO-PS;ES-PS 1;ES-PS 2;ES-PS 3;ES-PS4; MEDIA PSO-PS; MEDIA ES-PS 1; MEDIA ES-PS 2; MEDIA ES-PS 3; MEDIA ES-PS4">>result.csv

echo "PROBLEMA;SEMENTE;DELTA;MEDIA_ESPERADA;DIMENSAO;PSO-PS;ES-PS3; MEDIA PSO-PS; MEDIA ES-PS 3">>result.csv

for((dim=2; m<31; dim++))
do
	for((m=1; m<11; m=m+0.1))
	do
		for((d=1; d<11; d = d+0.1 ))
		do
			for(( f=1; f<15; f++ ))
			  do   
			   for(( s=1; s<31; s++ ))		  
				  do
				   ./teste $f $s $d $m $dim >> result.csv
				  done          
			  done
		done 
	done
done

cat README.txt
gedit result.csv
