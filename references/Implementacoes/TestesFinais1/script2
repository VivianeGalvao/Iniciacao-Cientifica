g++ -o teste *.cpp -std=c++0x

rm README.txt
rm resultS.csv

echo "Codigo Compilado"
f=0
s=0
d=0
m=0
dim=0

echo "PROBLEMA;SEMENTE;PSO;ES-1;ES-2;ES-3;ES-4;MEDIA PSO;MEDIA ES-1;MEDIA ES-2;MEDIA ES-3;MEDIA ES-4">>resultS.csv

for(( f=1; f<23; f++ ))
  do   
   for(( s=1; s<4; s++ ))		  
	  do
	   ./teste $f $s >> resultS.csv
	  done          
  done

gedit resultS.csv

